{
  "name": "collegeprep",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "test": "ng test",
    "build": "ng build --target=production --aot=true",
    "lint": "tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor --live=true",
    "live-integration-tests": "npm-run-all --parallel {1} e2e-live --race",
    "e2e-live": "ng e2e",
    "mock-integration-tests": "npm-run-all --parallel mock e2e-mock --race",
    "e2e-mock": "npm run pree2e && protractor --live=false",
    "mock": "ng serve --port=3000 --env=proxy",
    "local": "ng serve --port=3000 --environment=local --sourcemap=false",
    "dev": "ng serve --port=3000 --environment=proxy --proxy-config=config/dev.proxy.json --sourcemap=false",
    "qa": "ng serve --port=3000 --environment=proxy --proxy-config=config/qa.proxy.json --sourcemap=false",
    "staging": "ng serve --port=3000 --environment=proxy --proxy-config=config/staging.proxy.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "client-angular2-components": "git+ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/client-angular2-components",
    "core-js": "^2.4.1",
    "moment": "2.15.1",
    "ng2-bootstrap": "1.3.3",
    "rxjs": "^5.1.0",
    "ts-helpers": "1.1.1",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.32.3",
    "@angular/compiler-cli": "2.4.8",
    "@types/node": "~6.0.60",
    "bootstrap": "git+ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/client-pathevo-theme",
    "codelyzer": "~2.0.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
